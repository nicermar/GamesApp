<!DOCTYPE html>

<html>
<head>
   
 
    <meta name="viewport" content="width=device-width" />
    <title>Games & Toys</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/knockout")
  
    
</head>
<body>
    <div>
        @RenderBody()
    </div>
   
 
    <div class="alert alert-success alert-dismissible" role="alert">
        <button type="button" class="hide" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button><span class="alert-text"></span>
    </div>
    <div class="alert alert-info alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button><span class="alert-text"></span>
    </div>
    <div class="alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button><span class="alert-text"></span>
    </div>
    <div class="alert alert-danger " role="alert">
        <button type="button" class="close" data-hide="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button><span class="alert-text"></span>
    </div>
   
   
</body>
</html>
